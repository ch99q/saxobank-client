    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "saxobank",
    "client",
    "trading",
    "data",
    "banking",
    "portfolio"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "bun-types": "latest",
    "dotenv": "^17.2.3",
    "tsdown": "^0.15.6",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.2.23"
}
